{
  "name": "Sova Intel",
  "version": "0.15.5",
  "description": "Solana wallet analysis - analyze behavior, decode strategies, profit smarter",
  "main": "dist/index.js",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "dev:quiet": "cross-env QUIET_MODE=true ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "dev:verbose": "cross-env NODE_ENV=production ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "clean": "rimraf dist logs",
    "build:watch": "tsc --watch",
    "verify": "tsc --noEmit",
    "test:unit": "node --test -r ts-node/register -r tsconfig-paths/register src/core/services/helius-api-client.v2.test.ts src/core/services/helius-transaction-mapper.test.ts",
    "analyze": "ts-node -r ts-node/register src/scripts/helius-analyzer.ts",
    "similarity": "ts-node -r ts-node/register src/scripts/activityCorrelator.ts",
    "kpi-report": "ts-node -r ts-node/register src/scripts/kpi-comparison-report.ts",
    "behavior-analyzer": "ts-node -r ts-node/register src/scripts/wallet-behavior-analyzer.ts",
    "wallet-similarity": "ts-node -r ts-node/register src/scripts/walletSimilarity.ts",
    "start:wallet-analysis": "ts-node -r ts-node/register src/wallet_analysis/index.ts",
    "dev:wallet-analysis": "ts-node-dev -r tsconfig-paths/register --respawn src/wallet_analysis/index.ts",
    "pm2:start:wallet-analysis": "npm run build && pm2 start ecosystem.config.js",
    "pm2:start:backend": "npm run build && pm2 start ecosystem.backend.config.js --env production",
    "debug:mapper": "ts-node -r tsconfig-paths/register scripts/debug_mapper.ts",
    "test:queues": "npx ts-node src/queues/test-similarity-flow.ts",
    "test:integration": "npx ts-node -r tsconfig-paths/register src/queues/test-backend-integration.ts",
    "validate:deps": "npx ts-node -r tsconfig-paths/register scripts/validate-dependencies.ts",
    "clear-wallet": "npx ts-node -r tsconfig-paths/register scripts/clear-wallet-data.ts",
    "clear-queues": "npx ts-node scripts/clear-all-queues.ts",
    "clear-locks": "npx ts-node scripts/clear-stale-locks.ts",
    "clean-cache": "npx ts-node scripts/clean-wallet-cache.ts",
    "clean-logs": "npx ts-node scripts/clean-logs.ts",
    "emergency-stop": "pm2 stop all && npm run clear-queues && npm run clear-locks",
    "reset-system": "npm run emergency-stop && pm2 start ecosystem.config.js",
    "reset": "npm run build && pm2 restart all",
    "top-holders": "ts-node -r tsconfig-paths/register src/scripts/topTokenHolders.ts"
  },
  "keywords": [
    "crypto",
    "analysis",
    "wallet",
    "typescript",
    "data-verification"
  ],
  "author": "",
  "license": "ISC",
  "bin": {
    "mint-participants": "dist/scripts/mint-participants.js"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@nydiokar/autodoc": "^0.1.7",
    "@prisma/client": "^6.16.3",
    "@radix-ui/react-avatar": "^1.1.10",
    "@types/better-sqlite3": "^7.6.13",
    "@types/csv-stringify": "^1.4.3",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.2.0",
    "bignumber.js": "^9.3.1",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compute-cosine-similarity": "^1.0.0",
    "crypto-js": "^4.2.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "ioredis": "^5.8.2",
    "json-bigint": "^1.0.0",
    "p-limit": "^7.1.1",
    "papaparse": "^5.5.3",
    "react-wrap-balancer": "^1.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sonner": "^2.0.7",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "chalk": "5.3.0",
    "table": "^6.9.0",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "telegraf": "^4.15.3",
    "uuid": "^13.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/chai": "^5.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.3",
    "@types/json-bigint": "^1.0.4",
    "@types/lodash": "^4.17.18",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.7.0",
    "@types/papaparse": "^5.3.15",
    "@types/uuid": "^10.0.0",
    "chai": "^5.2.1",
    "cross-env": "^10.0.0",
    "mocha": "^11.7.2",
    "nodemon": "^3.1.0",
    "pm2": "^6.0.13",
    "prisma": "^6.16.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "glob": "^11.0.0",
    "multer": "2.0.1",
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1",
    "@nestjs/platform-express": {
      "multer": "2.0.1"
    }
  }
}
