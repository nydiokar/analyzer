{
  "name": "Sova Intel",
  "version": "0.11.5",
  "description": "Solana wallet analysis - analyze behavior, decode strategies, profit smarter",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "dev:quiet": "cross-env QUIET_MODE=true ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "dev:verbose": "cross-env NODE_ENV=production ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "clean": "rimraf dist data logs",
    "build:watch": "tsc --watch",
    "verify": "tsc --noEmit",
    "analyze": "ts-node -r ts-node/register src/scripts/helius-analyzer.ts",
    "similarity": "ts-node -r ts-node/register src/scripts/activityCorrelator.ts",
    "kpi-report": "ts-node -r ts-node/register src/scripts/kpi-comparison-report.ts",
    "behavior-analyzer": "ts-node -r ts-node/register src/scripts/wallet-behavior-analyzer.ts",
    "wallet-similarity": "ts-node -r ts-node/register src/scripts/walletSimilarity.ts",
    "start:wallet-analysis": "ts-node -r ts-node/register src/wallet_analysis/index.ts",
    "dev:wallet-analysis": "ts-node-dev -r tsconfig-paths/register --respawn src/wallet_analysis/index.ts",
    "pm2:start:wallet-analysis": "npm run build && pm2 start ecosystem.config.js",
    "pm2:start:backend": "npm run build && pm2 start ecosystem.backend.config.js --env production",
    "debug:mapper": "ts-node -r tsconfig-paths/register scripts/debug_mapper.ts",
    "test:queues": "npx ts-node src/queues/test-similarity-flow.ts",
    "test:integration": "npx ts-node -r tsconfig-paths/register src/queues/test-backend-integration.ts",
    "validate:deps": "npx ts-node -r tsconfig-paths/register scripts/validate-dependencies.ts"
  },
  "keywords": [
    "crypto",
    "analysis",
    "wallet",
    "typescript",
    "data-verification"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.3",
    "@nydiokar/autodoc": "^0.1.7",
    "@prisma/client": "^6.11.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@types/better-sqlite3": "^7.6.13",
    "@types/csv-stringify": "^1.4.3",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.1.0",
    "bignumber.js": "^9.3.0",
    "bullmq": "^5.56.0",
    "chalk": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compute-cosine-similarity": "^1.0.0",
    "crypto-js": "^4.2.0",
    "csv-stringify": "^6.5.2",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ioredis": "^5.6.1",
    "json-bigint": "^1.0.0",
    "p-limit": "^6.2.0",
    "papaparse": "^5.5.3",
    "react-wrap-balancer": "^1.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "sonner": "^2.0.5",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "table": "^6.9.0",
    "telegraf": "^4.15.3",
    "uuid": "^11.1.0",
    "winston": "^3.12.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^5.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^5.0.3",
    "@types/json-bigint": "^1.0.4",
    "@types/lodash": "^4.17.18",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.4",
    "@types/papaparse": "^5.3.15",
    "@types/uuid": "^10.0.0",
    "chai": "^5.2.0",
    "cross-env": "^7.0.3",
    "mocha": "^11.7.1",
    "nodemon": "^3.1.0",
    "pm2": "^6.0.6",
    "prisma": "^6.11.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "overrides": {
    "glob": "^11.0.0",
    "multer": "2.0.1"
  }
}
